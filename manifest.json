{
    "manifest_version": 2,

    "name": "AutoKayoko",
    "version": "0.1",
    "description": "Automated answering Kayoko's questions.",
    
    "content_scripts": [  
      { "run_at" :"document_end",
      "matches": ["https://meet.google.com/*"],
      "js": ["inject.js"]
      } ],
      
  "web_accessible_resources": ["script.js"],
    "permissions": [
      "activeTab"
    ],
    "browser_action": {
        "default_title": "AutoKayoko",
        "default_popup": "popup.html"
    }
}